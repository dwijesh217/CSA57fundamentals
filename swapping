#include<stdio.h>
int main()
{
	double first,second,temp;
	printf("enter the first number: ");
	scanf("%lf",&first);
	printf("enter the second number: ");
	scanf("%lf",&second);
	temp= first;
	first=second;
	second=temp;
	printf("after swapping,first number=%.2lf",first );
	printf("\n after swapping ,second number=%.2lf",second);
	return 0;
	
}

OUTPUT:
enter the first number: 23
enter the second number: 45
after swapping,first number=45.00
 after swapping ,second number=23.00
